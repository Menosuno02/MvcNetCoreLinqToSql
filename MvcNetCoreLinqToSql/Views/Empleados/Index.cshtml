@model List<Empleado>

<h1>Empleados LINQ</h1>
<table class="table">
    <thead>
        <tr>
            <th>Apellido</th>
            <th>Oficio</th>
            <th>Salario</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Empleado emple in Model)
        {
            <tr>
                <td>@emple.Apellido</td>
                <td>@emple.Oficio</td>
            </tr>
        }
    </tbody>
</table>